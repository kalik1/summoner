{
  "name": "quake-v2",
  "version": "0.1.0",
  "description": "Quake 3 Arena Backend Manager.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmetuysal/nest-hackathon-starter"
  },
  "author": "Ahmet Uysal",
  "license": "MIT",
  "scripts": {
    "build:server": "rimraf dist && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"src/server/test/**/*.ts\\\"",
    "start": "node dist/server/main.js",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "start:server:prod": "node dist/server/main",
    "ng": "ng",
    "start:client": "ng serve",
    "build:client": "ng build",
    "build": "rimraf dist && nest build && ng build",
    "build:start": "npm run build:all && node dist/server/main.js",
    "watch:client": "ng build --watch --configuration development",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:server:e2e": "jest --config test/server/jest-e2e.json",
    "migration:generate:local": "npm run clean && nest build && cross-env NODE_ENV=local typeorm migration:generate -n HackathonStarter",
    "migration:generate:production": "npm run clean && nest build && cross-env NODE_ENV=production typeorm migration:generate -n HackathonStarter",
    "migration:run:local": "npm run clean && nest build && cross-env NODE_ENV=local typeorm migration:run",
    "migration:run:production": "nest build && cross-env NODE_ENV=production typeorm migration:run",
    "migration:revert:local": "cross-env NODE_ENV=local typeorm migration:revert",
    "migration:revert:production": "cross-env NODE_ENV=production typeorm migration:revert",
    "serve:all": "concurrently \" npm run serve:source-and-book:docs \" \" npm run serve:dev \" ",
    "serve:browser:bundle": "browser-sync start --config browser-sync.js",
    "serve:dev": "concurrently  \" npm run serve:server:bundle \" \" npm run serve:browser:bundle \" \" npm run watch:server-and-browser:bundle \"",
    "serve:mydev": "concurrently  \" npm run watch:client \" \" npm run start:server:dev \" ",
    "serve:mydev1": "concurrently  \" npm run start:client \" ",
    "serve:dev:full": "concurrently \" npm run serve:dev \" \" npm run serve:source-and-book:docs \" ",
    "serve:server:bundle": "nodemon",
    "prestart:prod": "rimraf dist && tsc",
    "watch:browser:bundle": "ng build --extract-css --watch",
    "watch:server-and-browser:bundle": "concurrently \" npm run watch:server:bundle\" \" npm run watch:browser:bundle\"  ",
    "watch:server:bundle": "tsc -p tsconfig.json -w"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "cross-env NODE_ENV=development eslint --cache",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests"
    ],
    "{*.json,.{prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@nestjs/common": "^8.1.0",
    "@nestjs/core": "^8.1.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.0",
    "@nestjs/platform-socket.io": "^8.1.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.1.0",
    "@types/dockerode": "^3.3.0",
    "bcrypt": "^5.0.1",
    "browser-sync": "^2.27.5",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compodoc": "^0.0.41",
    "dockerode": "^3.3.1",
    "dotenv": "^8.6.0",
    "express-rate-limit": "^5.5.0",
    "helmet": "^4.6.0",
    "nanoid": "^3.1.29",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-query-stream": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.0",
    "typeorm": "^0.2.38",
    "typescript": "^4.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.1.0",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jasmine": "~3.8.0",
    "@types/jest": "26.0.24",
    "@types/nanoid": "^2.1.0",
    "@types/node": "14.17.25",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codelyzer": "~4.2.1",
    "concurrently": "^4.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.25.2",
    "gitbook-cli": "^2.3.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.8.0",
    "jest": "26.6.3",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "tsc-watch": "4.5.0",
    "tsconfig-paths": "3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src",
      "test"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky-disabled": {
    "hooks": {
      "pre-commit-disabled": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
